{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@reduxjs/toolkit": ["node_modules/@reduxjs/toolkit"],
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
, "src/App.js"  ]
}